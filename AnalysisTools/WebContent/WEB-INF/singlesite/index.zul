<zk>
	<div id="mainSingleSiteWindow" apply="org.zkoss.bind.BindComposer"
		viewModel="@id('vm') @init('org.pbtools.analysis.view.Index')" height="90%">
		<div sclass="page-header">
			<label value="Single-site Analysis" />
		</div>
			<tabbox id="tabBox" sclass="page-content">
			<tabs id="tabs">
				<tab label="Single-site Analysis"
					image="/images/find.png" />
			</tabs>
			<tabpanels id="tabPanels">
				<tabpanel id="specificationsPanel" 	style="overflow:auto">
					<include id="includeSpecifications" src="/user/analysis/singlesite/specifications.zul" />
				</tabpanel>
			</tabpanels>
		</tabbox>
	</div>
</zk>
